# editorconfig.org

root = true

[*]
charset                  = utf-8
end_of_line              = lf
insert_final_newline     = true
indent_style             = space
indent_size              = 4
trim_trailing_whitespace = true

[site/**/*.html]
insert_final_newline = false

[**/*.svg]
insert_final_newline = false

[**/*.json]
insert_final_newline = false
